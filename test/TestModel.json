{
	"namespace": "sports2000",
	"entityTypes": 
	{
		"Customer": 
		{
			"base": "DatabaseTable",
			"updateable": true,
			"table": "Customer",
			"baseType": "io.akera.UpdatableBusinessEntity",
			"properties": 
			{
				"CustNum": 
				{
					"type": "Edm.Int32"
				},

				"SalesRep": 
				{
					"type": "Edm.String"
				},

				"Name": 
				{
					"type": "Edm.String"
				},

				"Country": 
				{
					"type": "Edm.String"
				}
			},

			"methods": 
			{
				"getBalance": 
				{
					"type": "function",
					"params": 
					{
						"custNum": 
						{
							"type": "Edm.String",
							"direction": "in",
							"required": true
						},

						"balance": 
						{
							"type": "Edm.Float",
							"direction": "out"
						},

						"negative": 
						{
							"type": "Edm.Boolean",
							"direction": "out"
						}
					}
				},

				"setSalesRep": 
				{
					"type": "action",
					"params": 
					{
						"salesRep": 
						{
							"type": "Edm.String",
							"direction": "in",
							"required": true
						}
					}
				}
			},

			"key": 
			[
				"CustNum"
			]
		},

		"Order": 
		{
			"properties": 
			{
				"CustNum": 
				{
					"type": "Edm.Int32"
				},

				"OrderNum": 
				{
					"type": "Edm.Int32"
				},

				"SalesRep": 
				{
					"type": "Edm.String"
				},

				"OrderDate": 
				{
					"type": "Edm.Date"
				}
			},

			"key": 
			[
				"OrderNum"
			]
		},

		"OrderLine": 
		{
			"properties": 
			{
				"OrderNum": 
				{
					"type": "Edm.Int32"
				},

				"OrderLine": 
				{
					"type": "Edm.Int32"
				},

				"Qty": 
				{
					"type": "Edm.Int32"
				},

				"Amount": 
				{
					"type": "Edm.Int32"
				}
			},

			"key": 
			[
				"OrderNum",
				"OrderLine"
			]
		}
	},

	"entitySets": 
	{
		"Customers": 
		{
			"entityType": "sports2000.Customer"
		},

		"Orders": 
		{
			"entityType": "sports2000.Order"
		}
	}
}